{
  "name": "be-node-codebase",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "npx babel src -d dist",
    "start": "NODE_ENV=development nodemon src/index.js --exec \"node -r @babel/register\"",
    "serve": "NODE_ENV=production node dist/index.js",
    "lint": "npx eslint ./src --ext .js",
    "lint:fix": "npx eslint ./src --fix --ext .js",
    "s": "npx sequelize-cli",
    "db:migrate": "npm run s -- db:migrate",
    "db:seed": "npm run s -- db:seed",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run build"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "i": "^0.3.6",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mime-types": "^2.1.30",
    "multer": "^2.0.0-rc.1",
    "mysql2": "^2.2.5",
    "npm": "^7.13.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7"
  }
}
